<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rpframework.module.common.bottom.mapper.TokenMapper">
        <resultMap type="Token" id="RM">
                <id column="id" property="id"/>
                    <result column="upt_datetime" property="uptDatetime"/>
                    <result column="ticket" property="ticket"/>
                    <result column="access_token" property="accessToken"/>
                    <result column="gmt_datetime" property="gmtDatetime"/>
                    <result column="appsecret" property="appsecret"/>
                    <result column="app_id" property="appId"/>
        </resultMap>
        <resultMap type="Token" id="AliasRM">
                <id column="alias_token_id" property="id"/>
                    <result column="alias_token_upt_datetime" property="uptDatetime"/>
                    <result column="alias_token_app_id" property="appId"/>
                    <result column="alias_token_access_token" property="accessToken"/>
                    <result column="alias_token_ticket" property="ticket"/>
                    <result column="alias_token_gmt_datetime" property="gmtDatetime"/>
                    <result column="alias_token_appsecret" property="appsecret"/>
        </resultMap>
</mapper>