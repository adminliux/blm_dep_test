<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.rpframework.module.common.bottom.mapper.RoleMapper">
        <resultMap type="Role" id="RM">
                <id column="id" property="id"/>
                    <result column="parent_id" property="parentId"/>
                    <result column="name" property="name"/>
                    <result column="upt_datetime" property="uptDatetime"/>
                    <result column="gmt_datetime" property="gmtDatetime"/>
        </resultMap>
        <resultMap type="Role" id="AliasRM">
                <id column="alias_role_id" property="id"/>
                    <result column="alias_role_name" property="name"/>
                    <result column="alias_role_parent_id" property="parentId"/>
                    <result column="alias_role_gmt_datetime" property="gmtDatetime"/>
                    <result column="alias_role_upt_datetime" property="uptDatetime"/>
        </resultMap>
</mapper>