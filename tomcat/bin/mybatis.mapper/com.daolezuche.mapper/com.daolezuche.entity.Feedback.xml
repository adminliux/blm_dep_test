<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daolezuche.mapper.FeedbackMapper">
        <resultMap type="Feedback" id="RM">
                <id column="id" property="id"/>
                    <result column="way" property="way"/>
                    <result column="gmt_datetime" property="gmtDatetime"/>
                    <result column="type" property="type"/>
                    <result column="content" property="content"/>
                    <result column="upt_datetime" property="uptDatetime"/>
        </resultMap>
        <resultMap type="Feedback" id="AliasRM">
                <id column="alias_feedback_id" property="id"/>
                    <result column="alias_feedback_upt_datetime" property="uptDatetime"/>
                    <result column="alias_feedback_content" property="content"/>
                    <result column="alias_feedback_type" property="type"/>
                    <result column="alias_feedback_gmt_datetime" property="gmtDatetime"/>
                    <result column="alias_feedback_way" property="way"/>
        </resultMap>
</mapper>