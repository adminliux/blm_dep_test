<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.daolezuche.mapper.UserPositionMapper">
        <resultMap type="UserPosition" id="RM">
                <id column="id" property="id"/>
                    <result column="longitude" property="longitude"/>
                    <result column="upt_datetime" property="uptDatetime"/>
                    <result column="latitude" property="latitude"/>
                    <result column="user_id" property="userId"/>
                    <result column="gmt_datetime" property="gmtDatetime"/>
        </resultMap>
        <resultMap type="UserPosition" id="AliasRM">
                <id column="alias_user_position_id" property="id"/>
                    <result column="alias_user_position_gmt_datetime" property="gmtDatetime"/>
                    <result column="alias_user_position_latitude" property="latitude"/>
                    <result column="alias_user_position_longitude" property="longitude"/>
                    <result column="alias_user_position_user_id" property="userId"/>
                    <result column="alias_user_position_upt_datetime" property="uptDatetime"/>
        </resultMap>
</mapper>